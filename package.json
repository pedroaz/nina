{
  "name": "nina-explorer",
  "displayName": "nina-explorer",
  "description": "Code Visualizaton",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nina-explorer.nina-show-panel",
        "title": "Show",
        "category": "Nina"
      },
      {
        "command": "nina-explorer.nina-refresh-panel",
        "title": "Refresh",
        "category": "Nina"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "compile": "rollup -c && webpack --mode production",
    "watch": "concurrently \"rollup -c -w --bundleConfigAsCjs\" \"webpack --mode development --watch\""
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@tsconfig/svelte": "^5.0.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "rollup-plugin-minification": "^0.2.0",
    "rollup-plugin-svelte": "^7.2.2",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.1",
    "svelte-preprocess": "^6.0.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "rollup": "^4.18.0"
  }
}