  },
  "scripts": {
    "web-app:dev": "cd packages/web-app && pnpm dev",
    "landing-page:dev": "cd packages/landing-page && pnpm dev",
    "test:e2e": "pnpm playwright test",
    "test:e2e:watch": "nodemon --watch 'tests/**/*.spec.{ts,js}' --watch 'packages/web-app/**/*.{ts,tsx,js,jsx}' --watch 'packages/landing-page/**/*.{ts,tsx,js,jsx}' --watch 'packages/core/**/*.{ts,js}' --ext ts,tsx,js,jsx --exec 'pnpm playwright test'",
    "test:e2e:ui": "npx playwright test --ui",
    "genkit": "genkit start -- npx tsx --watch ./packages/core/llm/llm.ts"
  },
  "dependencies": {
    "mongoose": "^8.19.2",
    "tsx": "^4.20.6"
  }
